package com.cgpm.api.model.type;

public enum StatusType {
	
	ANALISE(1, "Em Análise"),
	ANALISE_REALIZADA(2, "Análise Realizada"),
	ANALISE_APROVADA(3, "Análise Aprovada"),
	INICIADO(4, "Iniciado"),
	PLANEJADO(5, "Planejado"),
	ANDAMENTO(6, "Em Andamento"),
	ENCERRADO(7, "Encerrado"),
	CANCELADO(8, "Cancelado");
	
	private Integer id;
	private String descricao;
	
	StatusType(Integer id, String descricao) {
		this.setId(id);
		this.setDescricao(descricao);
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}	
}

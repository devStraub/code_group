package com.cgpm.api.model.type;

public enum RiscoType {
	
	BAIXO("baixo_risco", "Baixo Risco"),
	MEDIO("medio_risco", "MÃ©dio Risco"),
	ALTO("alto_risco", "Alto Risco");
	
	private String id;
	private String descricao;
	
	RiscoType(String id, String descricao) {
		this.setId(id);
		this.setDescricao(descricao);
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}
		
}
